{
    "name": "mrkindy/n8n-laravel",
    "description": "Laravel package for integrating with n8n API using clean architecture patterns",
    "keywords": ["laravel", "n8n", "workflow", "automation", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ibrahim Abotaleb",
            "email": "admin@mrkindy.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "symfony/yaml": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "MrKindy\\N8NLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MrKindy\\N8NLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrKindy\\N8NLaravel\\N8NServiceProvider"
            ],
            "aliases": {
                "N8N": "MrKindy\\N8NLaravel\\Facades\\N8N"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
